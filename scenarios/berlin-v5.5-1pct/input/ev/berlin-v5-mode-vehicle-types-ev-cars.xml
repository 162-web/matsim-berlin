<?xml version="1.0" encoding="UTF-8"?>

<vehicleDefinitions xmlns="http://www.matsim.org/files/dtd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.matsim.org/files/dtd http://www.matsim.org/files/dtd/vehicleDefinitions_v2.0.xsd">

	<vehicleType id="car">
		<attributes>
			<attribute name="accessTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
			<attribute name="doorOperationMode" class="org.matsim.vehicles.VehicleType$DoorOperationMode">serial</attribute>
			<attribute name="egressTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
		</attributes>
		<capacity seats="1" standingRoomInPersons="0">

		</capacity>
		<length meter="7.5"/>
		<width meter="1.0"/>
		<engineInformation>
			<attributes>
				<attribute name="HbefaTechnology" class="java.lang.String">electricity</attribute>
				<attribute name="chargerTypes" class="java.util.Arrays$ArrayList">["default","specialChargerType"]</attribute>
				<attribute name="energyCapacityInKWhOrLiters" class="java.lang.Double">100.0</attribute>
				<attribute name="initialEnergy" class="java.lang.Double">100.0</attribute>
			</attributes>
		</engineInformation>
		<costInformation>

		</costInformation>
		<passengerCarEquivalents pce="1.0"/>
		<networkMode networkMode="car"/>
		<flowEfficiencyFactor factor="1.0"/>
	</vehicleType>

	<vehicleType id="freight">
		<attributes>
			<attribute name="accessTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
			<attribute name="doorOperationMode" class="org.matsim.vehicles.VehicleType$DoorOperationMode">serial</attribute>
			<attribute name="egressTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
		</attributes>
		<capacity seats="1" standingRoomInPersons="0">

		</capacity>
		<length meter="15.0"/>
		<width meter="1.0"/>
		<costInformation>

		</costInformation>
		<passengerCarEquivalents pce="3.5"/>
		<networkMode networkMode="car"/>
		<flowEfficiencyFactor factor="1.0"/>
	</vehicleType>

	<vehicleType id="ride">
		<attributes>
			<attribute name="accessTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
			<attribute name="doorOperationMode" class="org.matsim.vehicles.VehicleType$DoorOperationMode">serial</attribute>
			<attribute name="egressTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
		</attributes>
		<capacity seats="1" standingRoomInPersons="0">

		</capacity>
		<length meter="7.5"/>
		<width meter="1.0"/>
		<costInformation>

		</costInformation>
		<passengerCarEquivalents pce="1.0"/>
		<networkMode networkMode="car"/>
		<flowEfficiencyFactor factor="1.0"/>
	</vehicleType>

		

</vehicleDefinitions>